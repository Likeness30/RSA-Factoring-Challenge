#!/usr/bin/python3
import time

# Function to check if a number is prime
def is_prime(n):
    if n <= 1:
        return False
    if n <= 3:
        return True
    if n % 2 == 0 or n % 3 == 0:
        return False
    i = 5
    while i * i <= n:
        if n % i == 0 or n % (i + 2) == 0:
            return False
        i += 6
    return True

# Function to factorize the RSA number
def factorize_rsa(n):
    start_time = time.time()
    p = None
    q = None
    for i in range(2, n):
        if is_prime(i):
            if n % i == 0:
                p = i
                q = n // i
                break
    end_time = time.time()
    return p, q, end_time - start_time

#Main function
if __name__ == "__main__":
    # Example RSA number
    n = 3233

    # Attempt to factorize the RSA number
    p, q, time_taken = factorize_rsa(n)

    # Print the results
    if p and q:
        print("RSA number:", n)
        print("Prime factors:", p, q)
        print("Time taken:", time_taken, "seconds")
    else:
        print("Unable to factorize the RSA number within 5 seconds.")
